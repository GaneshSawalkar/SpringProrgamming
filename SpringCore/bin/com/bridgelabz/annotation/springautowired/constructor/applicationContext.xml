<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
               http://www.springframework.org/schema/context 
               http://www.springframework.org/schema/context/spring-context.xsd">
	<context:annotation-config />

	<bean id="axsax" class="com.bridgelabz.annotation.springautowired.constructor.address">
		<constructor-arg value="navi-mumbai" />
		<constructor-arg value="maharashtra" />
		<constructor-arg value="india" />
	</bean>

	<bean id="emp3" class="com.bridgelabz.annotation.springautowired.constructor.Employee" autowire="constructor">
		<constructor-arg value="1" type="int" />
		<constructor-arg value="ganu"></constructor-arg>
	</bean>

</beans>  